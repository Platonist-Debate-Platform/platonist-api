{
  "kind": "collectionType",
  "collectionName": "branches",
  "info": {
    "name": "Branch"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "lead": {
      "type": "string",
      "required": true
    },
    "previewImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "header": {
      "type": "component",
      "repeatable": false,
      "component": "page-content.jumbotron"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "page-content.text",
        "page-content.text-with-image",
        "page-content.text-with-list"
      ],
      "required": true
    },
    "previewLead": {
      "type": "text"
    },
    "page": {
      "model": "page"
    }
  }
}
