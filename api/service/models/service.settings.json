{
  "kind": "collectionType",
  "collectionName": "service",
  "info": {
    "name": "service"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "lead": {
      "type": "text",
      "required": true
    },
    "previewImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "header": {
      "type": "component",
      "repeatable": false,
      "component": "page-content.jumbotron",
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "page-content.text-with-image",
        "page-content.text",
        "page-content.text-with-list"
      ],
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "previewLead": {
      "type": "text"
    },
    "page": {
      "model": "page"
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "component": "page-content.contact-box"
    }
  }
}
